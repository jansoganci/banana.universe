  "analytics": {
    "events": {
      "paywall_viewed": {
        "description": "User viewed the paywall",
        "properties": ["variant", "placement_id", "user_segment"]
      },
      "product_selected": {
        "description": "User selected a subscription product",
        "properties": ["product_id", "variant", "selection_time"]
      },
      "cta_tapped": {
        "description": "User tapped the CTA button",
        "properties": ["product_id", "variant", "time_to_tap"]
      },
      "purchase_completed": {
        "description": "Purchase completed successfully",
        "properties": ["product_id", "variant", "revenue", "trial_started"]
      },
      "purchase_failed": {
        "description": "Purchase failed",
        "properties": ["product_id", "variant", "error_code", "error_message"]
      },
      "restore_tapped": {
        "description": "User tapped restore purchases",
        "properties": ["variant", "time_to_restore"]
      },
      "restore_completed": {
        "description": "Restore completed successfully",
        "properties": ["variant", "restored_products"]
      },
      "terms_tapped": {
        "description": "User tapped terms of service",
        "properties": ["variant", "time_to_terms"]
      },
      "privacy_tapped": {
        "description": "User tapped privacy policy",
        "properties": ["variant", "time_to_privacy"]
      }
    },
    "user_properties": [
      "device_type",
      "ios_version",
      "app_version",
      "user_segment",
      "previous_purchases",
      "session_count",
      "time_since_install"
    ],
    "session_properties": [
      "time_of_day",
      "day_of_week",
      "session_duration",
      "previous_screen",
      "network_type"
    ]
  },
  "experiments": {
    "conversion_rate": {
      "primary_metric": true,
      "target": 0.05,
      "minimum_detectable_effect": 0.20,
      "confidence_level": 0.95,
      "statistical_power": 0.80
    },
    "revenue_per_user": {
      "primary_metric": true,
      "target": 0.50,
      "minimum_detectable_effect": 0.20,
      "confidence_level": 0.95,
      "statistical_power": 0.80
    },
    "cta_tap_rate": {
      "secondary_metric": true,
      "target": 0.15,
      "minimum_detectable_effect": 0.25,
      "confidence_level": 0.95,
      "statistical_power": 0.80
    },
    "trial_start_rate": {
      "secondary_metric": true,
      "target": 0.08,
      "minimum_detectable_effect": 0.30,
      "confidence_level": 0.95,
      "statistical_power": 0.80
    }
  },
  "sample_size": {
    "minimum_per_variant": 2000,
    "recommended_per_variant": 5000,
    "maximum_duration_weeks": 6,
    "early_stopping_minimum": 1000
  },
  "compliance": {
    "apple_guidelines": {
      "no_external_payments": true,
      "apple_approved_links_only": true,
      "clear_error_messaging": true,
      "proper_accessibility": true,
      "wcag_aa_compliance": true
