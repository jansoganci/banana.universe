      "error_handling": true,
      "localization_support": true,
      "accessibility_compliance": true
    }
  },
  "technical_specs": {
    "swift_version": "5.9",
    "ios_version": "16.0",
    "dependencies": [
      "SwiftUI",
      "Adapty",
      "Foundation"
